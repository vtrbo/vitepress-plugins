{
  "name": "vitepress-plugin-runcode",
  "type": "module",
  "version": "0.2.0",
  "description": "Victor Bo's Vitepress Plugins RunCode.",
  "author": {
    "name": "Victor Bo",
    "email": "hi@vtrbo.cn"
  },
  "license": "MIT",
  "homepage": "https://github.com/vtrbo/vitepress-plugins/tree/main/packages/runcode",
  "keywords": [
    "vitepress",
